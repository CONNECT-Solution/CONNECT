<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="Maven" xmlns="http://maven.apache.org/DECORATION/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
    <bannerLeft>
        <name>CONNECT Solution</name>
        <src>http://www.connectopensource.org/sites/connectopensource.org/themes/connect/graphics/connect_logo.gif</src>
        <href>http://www.connectopensource.org/</href>
    </bannerLeft>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <custom>
        <fluidoSkin>
            <sideBarEnabled>true</sideBarEnabled>
            <gitHub>
                <projectId>CONNECT-Solution/CONNECT</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
            <googlePlusOne />
            <facebookLike />
            <twitter>
                <user>CONNECT_Project</user>
                <showUser>true</showUser>
                <showFollowers>true</showFollowers>
            </twitter>
            <googleSearch>
                <sitesearch />
            </googleSearch>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
        </fluidoSkin>
    </custom>

    <body>
        <menu ref="modules" inherit="top" />
        <menu ref="reports" inherit="top" />
    </body>
</project>