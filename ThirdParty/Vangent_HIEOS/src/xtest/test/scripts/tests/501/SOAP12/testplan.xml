                    <AdhocQuery id="urn:uuid:51224314-5390-4169-9b91-b1980040715a">
                        <Slot name="$uuid">
                            <ValueList>
                                <Value>('$uuid1$', '$uuid2$')</Value>
                            </ValueList>
                        </Slot>
                    </AdhocQuery>
                </query:AdhocQueryRequest>
            </Metadata>
            <UseId testdir="../../../testdata/500" id="Document01" step="submit_2doc_w_fol"
