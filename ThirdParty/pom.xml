<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
                      http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.connectopensource</groupId>
        <artifactId>CONNECTSolution</artifactId>
        <version>4.0.0-SNAPSHOT</version>
    </parent>
    <artifactId>ThirdParty</artifactId>
    <packaging>pom</packaging>
    <name>CONNECT Third Party</name>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-install-plugin</artifactId>
                    <version>2.4</version>
                </plugin>
            </plugins>
        </pluginManagement>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-install-plugin</artifactId>
                <executions>
                    <execution>
                        <id>javax.jms:jms</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <file>${basedir}/jms1.1/lib/javax.jms.jar</file>
                            <groupId>javax.jms</groupId>
                            <artifactId>jms</artifactId>
                            <version>1.1</version>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                            <createChecksum>true</createChecksum>
                        </configuration>
                    </execution>
                    <execution>
                        <id>javax.transaction:jta</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <file>${basedir}/JTA/javax.transaction.jar</file>
                            <groupId>javax.transaction</groupId>
                            <artifactId>jta</artifactId>
                            <version>1.0.1B</version>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                            <createChecksum>true</createChecksum>
                        </configuration>
                    </execution>
                    <execution>
                        <id>com.sun.jmx:jmxri</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <file>${basedir}/jmx-1_2_1-bin/lib/jmxri.jar</file>
                            <groupId>com.sun.jmx</groupId>
                            <artifactId>jmxri</artifactId>
                            <version>1.2.1</version>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                            <createChecksum>true</createChecksum>
                        </configuration>
                    </execution>
                    <execution>
                        <id>com.sun.jdmk:jmxtools</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <file>${basedir}/jmx-1_2_1-bin/lib/jmxtools.jar</file>
                            <groupId>com.sun.jdmk</groupId>
                            <artifactId>jmxtools</artifactId>
                            <version>1.2.1</version>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                            <createChecksum>true</createChecksum>
                        </configuration>
                    </execution>
                    <execution>
                        <id>com.sun.identity:opensso</id>
