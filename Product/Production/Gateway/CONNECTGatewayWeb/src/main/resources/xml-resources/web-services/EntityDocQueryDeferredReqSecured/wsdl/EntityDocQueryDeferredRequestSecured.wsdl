<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  Entity Document Query Deferred Request Secured WSDL
  -->
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="urn:gov:hhs:fha:nhinc:entitydocquerydeferredrequestsecured" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:query="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0" xmlns:nccommonentity="urn:gov:hhs:fha:nhinc:common:nhinccommonentity" xmlns:dq="http://www.hhs.gov/healthit/nhin" xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy" xmlns:sc="http://schemas.sun.com/2006/03/wss/server" xmlns:wspp="http://java.sun.com/xml/ns/wsit/policy" name="EntityDocQueryDeferredRequestSecured" targetNamespace="urn:gov:hhs:fha:nhinc:entitydocquerydeferredrequestsecured">
	<documentation>Entity Document Query Deferred Request Secured</documentation>
	<types>
		<xsd:schema>
			<xsd:import namespace="urn:oasis:names:tc:ebxml-regrep:xsd:query:3.0" schemaLocation="../schemas/ebRS/query.xsd"/>
			<xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonentity" schemaLocation="../schemas/nhinc/common/NhincCommonEntity.xsd"/>
			<xsd:import namespace="http://www.hhs.gov/healthit/nhin" schemaLocation="../schemas/nhin/QueryDocument.xsd"/>
		</xsd:schema>
	</types>
	<message name="RespondingGateway_CrossGatewayQueryRequestSecuredMessage">
		<part name="RespondingGateway_CrossGatewayQueryRequest" element="nccommonentity:RespondingGateway_CrossGatewayQuerySecuredRequest"/>
	</message>
	<message name="AcknowledgementMessage">
		<part name="RespondingGateway_CrossGatewayQueryResponse" element="dq:DocQueryAcknowledgement"/>
	</message>
	<portType name="EntityDocQueryDeferredRequestSecuredPortType">
		<operation name="RespondingGateway_CrossGatewayQuery">
			<input name="RespondingGateway_CrossGatewayQueryRequest" message="tns:RespondingGateway_CrossGatewayQueryRequestSecuredMessage"/>
			<output name="AcknowledgementMessage" message="tns:AcknowledgementMessage"/>
		</operation>
	</portType>
	<binding name="EntityDocQueryDeferredRequestSecuredBindingSoap" type="tns:EntityDocQueryDeferredRequestSecuredPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsp:PolicyReference URI="#RespondingGateway_Query_Binding_SoapPolicy"/>
		<operation name="RespondingGateway_CrossGatewayQuery">
			<soap:operation soapAction="urn:RespondingGateway_CrossGatewayQuery"/>
			<input name="RespondingGateway_CrossGatewayQueryRequest">
				<soap:body use="literal"/>
				<wsp:PolicyReference URI="#Entity_DocQueryDeferred_Binding_Soap_Input_Policy"/>
			</input>
			<output name="AcknowledgementMessage">
				<soap:body use="literal"/>
				<wsp:PolicyReference URI="#Entity_DocQueryDeferred_Binding_Soap_Output_Policy"/>
			</output>
		</operation>
	</binding>
	<service name="EntityDocQueryDeferredRequestSecured">
		<port name="EntityDocQueryDeferredRequestSecuredPortSoap" binding="tns:EntityDocQueryDeferredRequestSecuredBindingSoap">
			<soap:address location="https://localhost:${HttpsDefaultPort}/NhinConnect/EntityDocQueryDeferredRequestSecured"/>
		</port>
	</service>
	<wsp:Policy wsu:Id="RespondingGateway_Query_Binding_SoapPolicy">
		<wsp:ExactlyOne>
			<wsp:All>
				<wsaws:UsingAddressing xmlns:wsaws="http://www.w3.org/2006/05/addressing/wsdl"/>
				<sc:KeyStore wspp:visibility="private" aliasSelector="gov.hhs.fha.nhinc.callback.KeyStoreServerAliasSelector" callbackHandler="gov.hhs.fha.nhinc.callback.KeyStoreCallbackHandler"/>
				<sc:TrustStore wspp:visibility="private" callbackHandler="gov.hhs.fha.nhinc.callback.TrustStoreCallbackHandler"/>
				<sp:TransportBinding>
					<wsp:Policy>
						<sp:TransportToken>
							<wsp:Policy>
								<sp:HttpsToken>
									<wsp:Policy>
										<sp:RequireClientCertificate/>
									</wsp:Policy>
								</sp:HttpsToken>
							</wsp:Policy>
						</sp:TransportToken>
						<sp:Layout>
							<wsp:Policy>
								<sp:Strict/>
							</wsp:Policy>
						</sp:Layout>
						<sp:IncludeTimestamp/>
						<sp:AlgorithmSuite>
							<wsp:Policy>
								<sp:Basic128/>
							</wsp:Policy>
						</sp:AlgorithmSuite>
					</wsp:Policy>
				</sp:TransportBinding>
				<sp:EndorsingSupportingTokens>
					<wsp:Policy>
						<sp:SamlToken sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient">
							<wsp:Policy>
								<sp:WssSamlV20Token11/>
							</wsp:Policy>
						</sp:SamlToken>
					</wsp:Policy>
				</sp:EndorsingSupportingTokens>
				<sp:Wss11>
					<wsp:Policy>
						<sp:MustSupportRefKeyIdentifier/>
						<sp:MustSupportRefIssuerSerial/>
						<sp:RequireSignatureConfirmation/>
					</wsp:Policy>
				</sp:Wss11>
			</wsp:All>
		</wsp:ExactlyOne>
	</wsp:Policy>
	<wsp:Policy wsu:Id="Entity_DocQueryDeferred_Binding_Soap_Input_Policy">
		<wsp:ExactlyOne>
			<wsp:All>
      </wsp:All>
		</wsp:ExactlyOne>
	</wsp:Policy>
	<wsp:Policy wsu:Id="Entity_DocQueryDeferred_Binding_Soap_Output_Policy">
		<wsp:ExactlyOne>
			<wsp:All>
      </wsp:All>
		</wsp:ExactlyOne>
	</wsp:Policy>
</definitions>
