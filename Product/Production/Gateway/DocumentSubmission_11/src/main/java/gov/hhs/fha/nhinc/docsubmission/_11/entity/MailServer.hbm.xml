<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.hhs.fha.nhinc.docsubmission._11.entity">
  <class name="MailServer" table="mailserver">
	<id name="id" column="id" type="int">
		<generator class="increment" />
	</id>
	<property name="attributename" column="attributename" type="string" length="64" not-null="false"/>
	<property name="attributevalue" column="attributevalue" type="string" length="64" not-null="false"/>
		<property name="type" column="type" type="string" length="64" not-null="false"/>
  </class>
</hibernate-mapping>