    <import resource="classpath:META-INF/cxf/cxf.xml" />
    <import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
    <import resource="classpath:CONNECT-interceptor-beans.xml" />

    <!-- Document Submission -->

    <!-- Nhin services -->
    <bean id="nhinDocSubmissionOrchImpl" class="gov.hhs.fha.nhinc.docsubmission.nhin.NhinDocSubmissionOrchImpl" />
    <bean id="nhinXDR" class="gov.hhs.fha.nhinc.docsubmission._11.nhin.NhinXDR">
       <property name="orchestratorImpl">
            <ref bean="nhinDocSubmissionOrchImpl"/>
        </property>
    </bean>
    <jaxws:endpoint xmlns:nds="urn:ihe:iti:xdr:2007" id="NhinDocSubmission" address="/DocumentRepositoryXDR_Service"
        serviceName="nds:DocumentRepositoryXDR_Service" endpointName="nds:DocumentRepositoryXDR_Port_Soap"
        implementorClass="gov.hhs.fha.nhinc.docsubmission._11.nhin.NhinXDR"
        implementor="#nhinXDR"
        wsdlLocation="classpath:wsdl/NhinXDR.wsdl">
        <jaxws:properties>
            <entry key="ws-security.callback-handler" value="gov.hhs.fha.nhinc.callback.cxf.CXFSAMLCallbackHandler" />
            <entry key="ws-security.signature.properties" value="keystore.properties" />
            <entry key="ws-security.encryption.properties" value="truststore.properties" />
            <entry key="ws-security.saml2.validator">
                <bean class="gov.hhs.fha.nhinc.callback.cxf.CONNECTSamlAssertionValidator" />
            </entry>
            <entry key="mtom-enabled" value="true" />
        </jaxws:properties>

        <jaxws:inInterceptors>
            <ref bean="securityInInterceptor" />
        </jaxws:inInterceptors>
        <jaxws:handlers>
            <ref bean="SOAPHeaderHandler" />
            <ref bean="TransactionHandler" />
        </jaxws:handlers>
    </jaxws:endpoint>

    <!-- Entity services -->
    <bean id="entityDocSubmissionOrchImpl" class="gov.hhs.fha.nhinc.docsubmission.entity.EntityDocSubmissionOrchImpl" />

    <bean id="entityXDRUnsecured" class="gov.hhs.fha.nhinc.docsubmission._11.entity.EntityDocSubmissionUnsecured">
        <property name="orchestratorImpl">
            <ref bean="entityDocSubmissionOrchImpl" />
        </property>
    </bean>
    <jaxws:endpoint xmlns:eds="urn:gov:hhs:fha:nhinc:nhincentityxdr" id="EntityDocSubmissionUnsecured"
        address="/EntityService/EntityDocSubmissionUnsecured" serviceName="eds:EntityXDR_Service"
        endpointName="eds:EntityXDR_Port"
        implementorClass="gov.hhs.fha.nhinc.docsubmission._11.entity.EntityDocSubmissionUnsecured"
        implementor="#entityXDRUnsecured"
        wsdlLocation="classpath:wsdl/EntityXDR.wsdl">
        <jaxws:handlers>
            <ref bean="TransactionHandler" />
        </jaxws:handlers>
        <jaxws:properties>
            <entry key="mtom-enabled" value="true" />
        </jaxws:properties>
    </jaxws:endpoint>
    
    <bean id="entityXDRSecured" class="gov.hhs.fha.nhinc.docsubmission._11.entity.EntityDocSubmissionSecured">
        <property name="orchestratorImpl">
            <ref bean="entityDocSubmissionOrchImpl" />
        </property>
    </bean>
    <jaxws:endpoint xmlns:edss="urn:gov:hhs:fha:nhinc:nhincentityxdrsecured" id="EntityDocSubmissionSecured"
