<?xml version="1.0" encoding="UTF-8"?>
<!--
  Nhinc Proxy Document Retrieve Deferred Response Secured
  -->
<definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/" 
             name="NhincProxyDocRetrieveDeferredResponseSecured"
             targetNamespace="urn:gov:hhs:fha:nhinc:nhincproxydocretrievesecuredresponse"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:ihe="urn:ihe:iti:xds-b:2007"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:wsaw="http://www.w3.org/2006/05/addressing/wsdl"
             xmlns:wsaws="http://www.w3.org/2005/08/addressing"
             xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
             xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
             xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy"
             xmlns:sc="http://schemas.sun.com/2006/03/wss/client"
             xmlns:wspp="http://java.sun.com/xml/ns/wsit/policy"
             xmlns:dr="http://www.hhs.gov/healthit/nhin"
             xmlns:wsoma="http://schemas.xmlsoap.org/ws/2004/09/policy/optimizedmimeserialization"
             xmlns:nccommonproxy="urn:gov:hhs:fha:nhinc:common:nhinccommonproxy"
             xmlns:tns="urn:gov:hhs:fha:nhinc:nhincproxydocretrievesecuredresponse">
    <documentation>Nhinc Proxy document Retrieve Deferred Response Secured</documentation>
    <types>
        <xsd:schema>
            <xsd:import namespace="urn:ihe:iti:xds-b:2007" schemaLocation="../schemas/ihe/XDS.b_DocumentRepository.xsd"/>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonproxy" schemaLocation="../schemas/nhinc/common/NhincCommonProxy.xsd"/>
            <xsd:import namespace="urn:gov:hhs:fha:nhinc:gateway:samltokendata" schemaLocation="../schemas/nhinc/gateway/SamlTokenData.xsd"/>
            <xsd:import namespace="http://www.hhs.gov/healthit/nhin" schemaLocation="../schemas/nhin/RetrieveDocument.xsd"/>
        </xsd:schema>
    </types>
    <message name="CrossGatewayRetrieveResponseMessage">
        <part name="body" element="nccommonproxy:RespondingGateway_CrossGatewayRetrieveSecuredResponse"/>
    </message>
    <message name="CrossGatewayRetrieveAcknowledgementMessage">
        <part name="body" element="dr:DocRetrieveAcknowledgement"/>
    </message>
    <portType name="NhincProxyDocRetrieveDeferredResponseSecuredPortType">
        <operation name="CrossGatewayRetrieveResponse">
            <input name="CrossGatewayRetrieveResponse" message="tns:CrossGatewayRetrieveResponseMessage"/>
            <output name="CrossGatewayRetrieveAcknowledgement" message="tns:CrossGatewayRetrieveAcknowledgementMessage"/>
        </operation>
    </portType>
    <binding name="NhincProxyDocRetrieveDeferredResponseSecuredBindingSoap" type="tns:NhincProxyDocRetrieveDeferredResponseSecuredPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsp:PolicyReference URI="#RespondingGateway_Retrieve_Binding_SoapPolicy"/>
        <operation name="CrossGatewayRetrieveResponse">
            <soap:operation soapAction="urn:CrossGatewayRetrieveResponse"/>
            <input name="CrossGatewayRetrieveResponse">
                <soap:body use="literal"/>
                <wsp:PolicyReference URI="#RespondingGateway_Retrieve_Binding_Soap_Input_Policy"/>
            </input>
            <output name="CrossGatewayRetrieveAcknowledgement">
                <soap:body use="literal"/>
                <wsp:PolicyReference URI="#RespondingGateway_Retrieve_Binding_Soap_Output_Policy"/>
            </output>
        </operation>
    </binding>
    <service name="NhincProxyDocRetrieveDeferredResponseSecured">
        <port name="NhincProxyDocRetrieveDeferredResponseSecuredPortSoap" binding="tns:NhincProxyDocRetrieveDeferredResponseSecuredBindingSoap">
            <soap:address location="https://localhost:${HttpsDefaultPort}/NhinConnect/NhincProxyDocRetrieveDeferredResponseSecured"/>
        </port>
    </service>
    <wsp:Policy wsu:Id="RespondingGateway_Retrieve_Binding_SoapPolicy">
        <wsp:ExactlyOne>
            <wsp:All>
                <sc:KeyStore wspp:visibility="private" aliasSelector="gov.hhs.fha.nhinc.callback.KeyStoreClientAliasSelector" callbackHandler="gov.hhs.fha.nhinc.callback.KeyStoreCallbackHandler"/>
                <sc:TrustStore wspp:visibility="private" certSelector = "gov.hhs.fha.nhinc.callback.TrustStoreAliasSelector" callbackHandler="gov.hhs.fha.nhinc.callback.TrustStoreCallbackHandler"/>
                <sc:CallbackHandlerConfiguration wspp:visibility="private">
                    <sc:CallbackHandler name="samlHandler" classname="gov.hhs.fha.nhinc.callback.SamlCallbackHandler"/>
                </sc:CallbackHandlerConfiguration>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
    <wsp:Policy wsu:Id="RespondingGateway_Retrieve_Binding_Soap_Input_Policy">
        <wsp:ExactlyOne>
            <wsp:All>
                <wsoma:OptimizedMimeSerialization/>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
    <wsp:Policy wsu:Id="RespondingGateway_Retrieve_Binding_Soap_Output_Policy">
        <wsp:ExactlyOne>
            <wsp:All>
                <wsoma:OptimizedMimeSerialization/>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
</definitions>
