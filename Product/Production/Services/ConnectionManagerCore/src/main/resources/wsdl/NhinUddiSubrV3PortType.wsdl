   xmlns:tns="urn:uddi-org:subr_v3_portType" 
   xmlns:uddi="urn:uddi-org:api_v3" 
   xmlns:uddi_subr="urn:uddi-org:subr_v3" 
   targetNamespace="urn:uddi-org:subr_v3_portType">
       
